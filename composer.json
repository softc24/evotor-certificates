{
    "name": "softc/evotor-certificates",
    "description": "Provides access to Evotor Certificates app API",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "SoftC\\Evotor\\Certificates\\Api\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoftC\\Evotor\\Certificates\\Api\\Tests\\": "tests/"
        }
    },
    "archive": {
        "exclude": [
            "tests/",
            "vendor/",
            "nbproject/",
            ".github/"
        ]
    },
    "require": {
        "php": ">=7.4",
        "psr/http-client-implementation": "^1.0",
        "laminas/laminas-hydrator": "^4.5"
    },
    "require-dev": {
        "php-http/message": "^1.0",
        "php-http/mock-client": "^1.0",
        "phpstan/phpstan": "^1.5",
        "phpunit/phpunit": "^9.5",
        "php-http/curl-client": "^2.2",
        "laminas/laminas-diactoros": "^2.17"
    },
    "authors": [
        {
            "name": "Aleksandr Soloshenko",
            "email": "capcom@soft-c.ru"
        }
    ]
}